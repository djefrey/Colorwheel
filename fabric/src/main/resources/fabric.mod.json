{
  "schemaVersion": 1,
  "id": "colorwheel",
  "version": "${version}",
  "name": "Colorwheel",
  "description": "Allows you to use Iris Shaders with Flywheel",
  "authors": [
    "djefrey"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.djefrey.colorwheel.fabric.ClrwlFabric"
    ],
    "client": [
      "dev.djefrey.colorwheel.fabric.client.ClrwlFabricClient"
    ]
  },
  "mixins": [
    "colorwheel.mixins.iris.json",
    "colorwheel.mixins.flw.json",
    "colorwheel.mixins.mc.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.5",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*",
    "flywheel": ">=1.0.0",
    "iris": ">=1.8.0"
  },
  "suggests": {
    "colorwheel_patcher": ">=0.2.3"
  }
}
